        <div id="update" style="display: block">
            <form class="addOrderEquipment">
                <legend><strong>Add OrderEquipment</strong></legend>
                    <div class="add-OrderEquipment-container">
                        <label for="filter-user" class="filter-input-label">Order ID</label>
                        <div class="filter-input-element">
                            <select id="filter-user" class="filter-input" name="filter-user">
                                <option selected value="">Choose Order ID</option>

                                {{#each data1}}
                                <option name = "add-user-drop" value="{{this.orderID}}">{{this.orderID}}</option>
                                {{/each}}
                                

                            </select>
                        </div>
                    </div>
                    
                    <div class="add-OrderEquipment-container">
                        <label for="filter-user" class="filter-input-label">Equipment ID</label>
                        <div class="filter-input-element">
                            <select id="filter-user" class="filter-input" name="filter-user">
                            <option selected value="">Choose equipment ID</option>
                                {{#each data2}}
                                <option name = "add-user-drop" value="{{this.equipmentID}}">{{this.equipmentID}}</option>
                                {{/each}}
                            </select>
                        </div>
                    </div>  

                    <div class="add-OrderEquipment-container">
                        <label for="filter-text" class="filter-input-label">Equipment Amount</label>
                        <div class="filter-input-element">
                            <input type="text" name="filter-text" id="filter-text" class="filter-input">
                        </div>
                    </div>
              
                <input class="btn" type="submit" id="AddOrderEquipment" value="Update OrderEquipment">
                <input class="btn" type="button" value="Cancel" >
            </form> 
        </div><!-- update -->

