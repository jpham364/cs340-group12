                                <option name = "add-user-drop" value="{{this.equipmentID}}">{{this.equipmentID}}</option>
                                {{/each}}
                            </select>
                        </div>
                    </div>

                    <div class="update-amount-container">
                        <label for="filter-text" class="filter-input-label">Update Amount</label>
