<main class="content">
            <aside class="filter-equipment">
                <h2>OrderEquipment</h2>


                <div class="table-contents">
                    <table border="5" cellpadding="5" id="Order-Equipment-table">
                    
                        <thead>

                            <tr>
                                {{#each data.[0]}}
                                    <th>
                                        {{@key}}

                                    </th>
                                {{/each}}
                                <th>Deletes</th>
                            </tr>
                            
                        </thead>

                        <!-- Body section -->
                        <tbody>

                            {{#each data}}
                            {{!-- received this idea from ChatGPT --}}
                            <tr data-value=({{this.orderID}} + {{this.equipmentID}})>
                                <td>{{this.orderID}}</td>
                                <td>{{this.equipmentID}}</td>
                                <td>{{this.amount}}</td>
                                <td>{{this.cost}}</td>
                                <td><button onclick="deleteOrderEquipment({{this.orderID}}, {{this.equipmentID}})">Delete</button></td>
                            </tr>
                            {{/each}}

                        </tbody>

                    </table>
                    <script src="./js/OrderEquipment/delete_OrderEquipment.js"></script>
                </div>
            </aside>
        </main>


        <p>&nbsp;</p> <!-- WHITE SPACE-->

        <div id="update" style="display: block">
            <form id="add-Order-Equipment-form-ajax" class="addOrderEquipment">
                <legend><strong>Add OrderEquipment</strong></legend>
                    <div class="add-OrderEquipment-container">
                        <label for="filter-user" class="filter-input-label">Order ID</label>
                        <div class="filter-input-element">
                            <select id="orderIDSelect" class="filter-input" name="filter-user">
                               
                                <option selected value="">Select Order ID</option>
                                {{#each data1}}
                                <option name = "add-user-drop" value="{{this.orderID}}">{{this.orderID}}</option>
                                {{/each}}
                                

                            </select>
                        </div>
                    </div>
                    
                    <div class="add-OrderEquipment-container">
                        <label for="filter-user" class="filter-input-label">Equipment</label>
                        <div class="filter-input-element">
                            <select id="equipmentIDSelect" class="filter-input" name="filter-user">
                            <option selected value="">Select equipment</option>
                                {{#each data2}}
                                <option name = "add-user-drop" value="{{this.equipmentID}}">{{this.equipmentName}}</option>
                                {{/each}}
                            </select>
                        </div>
                    </div>  

                    <div class="add-OrderEquipment-container">
                        <label for="filter-text" class="filter-input-label">Equipment Amount</label>
                        <div class="filter-input-element">
                            <input type="text" name="input-amount" id="input-amount" class="filter-input">
                        </div>
                    </div>
              
                <input class="btn" type="submit" id="AddOrderEquipment" value="Add OrderEquipment">
                <input class="btn" type="button" value="Cancel" >
            </form> 
            <script src="./js/OrderEquipment/add_OrderEquipment.js"></script>
        </div><!-- update -->

        <p>&nbsp;</p> <!-- WHITE SPACE-->


        <div id="update" style="display: block">


            <form id="update-equipment-id-form-ajax" class="addOrderEquipment">
                <legend><strong>Update Equipment</strong></legend>
                    <div class="add-OrderEquipment-container">
                        <label for="filter-user" class="filter-input-label">Select Order ID</label>
                        <div class="filter-input-element">
                            <select id="updateOIDSelect" class="filter-input" name="filter-user">
                                <option selected value="">Select Order ID</option>
                                {{#each data1}}
                                    <option name = "add-user-drop" value="{{this.orderID}}">{{this.orderID}}</option>
                                {{/each}}
                            </select>
                        </div>
                    </div>  
                    
                    <div class="add-OrderEquipment-container">
                        <label for="filter-user" class="filter-input-label">Select Equipment</label>
                        <div class="filter-input-element">
                            <select id="updateEIDSelect" class="filter-input" name="filter-user">
                            <option selected value="">Select Equipment</option>
                                {{#each data2}}
                                <option name = "add-user-drop" value="{{this.equipmentID}}">{{this.equipmentID}}</option>
                                {{/each}}
                            </select>
                        </div>
                    </div>

                    <div class="add-OrderEquipment-container">
                        <label for="filter-user" class="filter-input-label">Reassign Equipment</label>
                        <div class="filter-input-element">
                            <select id="updateEIDAssign" class="filter-input" name="filter-user">
                                {{#each data2}}
                                <option name = "add-user-drop" value="{{this.equipmentID}}">{{this.equipmentID}}</option>
                                {{/each}}
                            </select>
                        </div>
                    </div>

                <input class="btn" type="submit" id="UpdateOrderEquipment" value="Update EquipmentID">
                <input class="btn" type="button" value="Cancel" >
            </form> 

            <script src="./js/OrderEquipment/update_OrderEquipment.js"></script>
        </div><!-- update -->

        <p>&nbsp;</p> <!-- WHITE SPACE-->

        <div id="update" style="display: block">
            
            <form id="update-amount-form-ajax" class="updateAmount">
